{
  "resource_limits": {
    "user_space_size": "2560MB"
  },
  "process": {
    "default_stack_size": "40MB",
    "default_heap_size": "320MB",
    "default_mmap_size": "960MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "mount": [
    {
      "options": {
        "layers": [
          {
            "options": {
              "MAC": "ef-dc-79-c7-be-a3-46-f0-85-e8-78-0d-c6-b3-8a-8e"
            },
            "source": "./build/mount/__ROOT",
            "target": "/",
            "type": "sefs"
          },
          {
            "source": "./run/mount/__ROOT",
            "target": "/",
            "type": "sefs"
          }
        ]
      },
      "target": "/",
      "type": "unionfs"
    },
    {
      "source": ".",
      "target": "/host",
      "type": "hostfs"
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}
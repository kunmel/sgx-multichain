{
  "resource_limits": {
    "user_space_size": "2560MB"
  },
  "process": {
    "default_stack_size": "40MB",
    "default_heap_size": "320MB",
    "default_mmap_size": "960MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "mount": [
    {
      "options": {
        "layers": [
          {
            "options": {
              "MAC": "f8-1f-e4-8b-a3-1f-0a-09-eb-2d-35-ed-13-57-a0-db"
            },
            "source": "./build/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          },
          {
            "source": "./run/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          }
        ]
      },
      "target": "/",
      "type": "unionfs"
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}
{
  "resource_limits": {
    "kernel_space_heap_size": "320MB",
    "kernel_space_stack_size": "10MB",
    "user_space_size": "2560MB",
    "max_num_of_threads": 32
  },
  "process": {
    "default_stack_size": "40MB",
    "default_heap_size": "320MB",
    "default_mmap_size": "960MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "metadata": {
    "product_id": 0,
    "version_number": 0,
    "debuggable": true
  },
  "mount": [
    {
      "target": "/",
      "type": "unionfs",
      "options": {
        "layers": [
          {
            "target": "/",
            "type": "sefs",
            "source": "./build/mount/__ROOT",
            "options": {
              "MAC": ""
            }
          },
          {
            "target": "/",
            "type": "sefs",
            "source": "./run/mount/__ROOT"
          }
        ]
      }
    },
    {
      "target": "/host",
      "type": "hostfs",
      "source": "."
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}
